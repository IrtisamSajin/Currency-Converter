{
    "manifest_version": 3,
    "name": "Currency Converter",
    "description": "Convert Currencies with current exchange rate",
    "version": "1.1",
    "action": {
      "default_popup": "popup/index.html"
    },
    "icons": {
      "16": "images/Icon_16.png",
      "32": "images/Icon_32.png",
      "48": "images/Icon_48.png",
      "128": "images/Icon_128.png"
    },
    "permissions":["storage"]
}